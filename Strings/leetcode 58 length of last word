class Solution {
    public int lengthOfLastWord(String s) {
        String st=s.trim();
        if(st.length()==0)return 0;
        int count=0;
        for(int i=0;i<st.length();i++)
        {
            char c=st.charAt(i);
            int a=c;
            if(i>0 && a==32)
            count=0;
            else
            {
                count++;
            }

        }
        return count;
        
    }
}
