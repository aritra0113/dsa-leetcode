class Solution {
    public int rob(int[] nums) {

        int dp[]=new int[nums.length];
         dp[0]=nums[0];
        for(int i=1;i<nums.length;i++)
        {
            int pick=(i>1)?nums[i]+dp[i-2]:nums[i];

            int right=dp[i-1];

           int max=Math.max(pick,right);
            dp[i]=max;
        }
        return dp[nums.length-1];
        
    }
}
